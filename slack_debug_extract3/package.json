  "private": true,
  "scripts": {
    "print-version": "ts-node --transpile-only ./build/version/index.ts --print",
    "preinstall": "node script/force-yarn.js",
    "postinstall": "./node_modules/.bin/patch-package && ts-node --transpile-only ./build/postinstall.ts",
    "patch:package": "patch-package",
    "patch:package-mac": "patch-package --patch-dir patches-mac",
    "prepare": "husky install && ts-node --transpile-only ./build/bootstrap-cli.ts",
    "package": "cross-env NODE_ENV=production ts-node --transpile-only ./build/package-cli.ts",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:locales": "ts-node --transpile-only ./ci/common/mas-supported-locale.ts",
    "format-all:check": "prettier --check build ci script spec src webpack",
    "format-all": "prettier --write build ci script spec src webpack",
    "prestart": "npm run build:dev",
    "start": "npm run start:app --",
    "start:app": "cross-env DEBUG='logger\\:*,electron-spellchecker\\:*' NODE_OPTIONS=--trace-deprecation electron . -r . --inspect",
    "start:appx": "cross-env DEBUG='logger\\:*,electron-spellchecker\\:*' NODE_OPTIONS=--trace-deprecation electronmsix . -r . --inspect",
    "test": "jest",
    "test:webpack": "jest --config ./webpack/spec/jest.json",
    "test:dependencies": "dependency-cruise --validate .dependency-cruiser.js ./src",
    "compile": "tsc -b ./src/common/tsconfig.json ./src/boot/tsconfig.json ./src/preload/tsconfig.json ./src/browser/tsconfig.json ./src/renderer/tsconfig.json ./webpack/tsconfig.json ./build/tsconfig.json ./spec/jest/tsconfig.json && tsc --noEmit --project ./spec/old-joe/tsconfig.json",
    "clean": "ts-node --transpile-only ./script/clean.ts",
    "build": "tsc -b ./webpack/tsconfig.json && node ./.webpack/runner.js",
    "build:dev": "npm run build",
    "build:prod": "npm run build -- --prod",
    "licenses": "ts-node --transpile-only ./build/generate-licenses-cli.ts",
    "copyright": "ts-node --transpile-only ./build/check-copyright-cli.ts",
    "dependencies": "depcruise -X \"node_modules\" --output-type dot src | dot -T svg > dependencygraph.svg",
    "dependencies:csv": "npx license-checker --csv --out ./licenses.csv",
    "changelog": "ts-node --transpile-only ./script/changelog.ts",
    "extract-strings": "ts-node --transpile-only ./script/extract-strings.ts",
    "codesign": "ts-node --transpile-only ./build/cli/codesign.ts",
    "export-types": "ts-node --transpile-only ./build/cli/export-types/index.ts",
    "cherry": "ts-node --transpile-only ./build/cli/cherrypick.ts",
    "update-server": "ts-node --transpile-only ./spec/e2e/utils/fake-update-server.ts",
    "https-auth-proxy": "ts-node --transpile-only ./spec/e2e/utils/https-auth-proxy.ts",
    "release-artifact-notification": "ts-node --transpile-only ./ci/common/release-artifact-notification.ts",
    "electron-versions": "electron-versions -f \">3.999\" -l 1000 -p alpha,beta,eta --write-markdown --write-json",
    "packagecloud": "ts-node --transpile-only ./build/cli/packageCloud/index.ts",
    "sentry-release": "ts-node --transpile-only ./build/sentry-release.ts"
  },
